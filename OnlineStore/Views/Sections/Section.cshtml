@page "{@id}"
@model Section

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Раздел @Model.Name</title>
</head>
<body>
<h1>Разнообразный ассортимент электроники!</h1>
<br />
@for (int i = 0; i < Model.Goods.Count; i++)
{
    <h2>Товар № @(i + 1)</h2>
    <p>@Model.Goods[i]</p>
}
</body>
</html>